  "contributors": [],
  "description": "A collection of JavaScript software development configurations and cli tools",
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "MIT",
  "name": "@wavesrcool/dev",
  "repository": {
    "directory": "packages/dev",
    "type": "git",
    "url": "https://github.com/wavesrcool/library.git"
  },
  "type": "commonjs",
  "version": "0.1.6",
  "bin": {
    "wavesrcool-configure-husky": "lib/cli/wavesrcool-configure-husky/main.js",
    "wavesrcool-compose-library": "lib/cli/wavesrcool-compose-library/main.js",
    "wavesrcool-library": "lib/cli/wavesrcool-library/main.js",
    "wavesrcool-rename-files-src": "lib/cli/wavesrcool-rename-files-src/main.js",
    "wavesrcool-rename-files-edit": "lib/cli/wavesrcool-rename-files-edit/main.js"
  },
  "files": [
    "./lib"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "w": "tsc -w",
    "0l": "rm -rf lib",
    "0t": "rm -f tsconfig*tsbuildinfo",
    "c": "yarn 0l && yarn 0t && tsc --project tsconfig.build.json",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings=0 --fix",
    "build": "yarn lint && yarn c && yarn executable-scripts",
    "prepack": "yarn build",
    "test": "yarn jest",
    "executable-scripts": "chmod u+x ./lib/cli/**/*.js"
  },
  "dependencies": {
    "@types/glob": "^8.0.0",
    "@types/yargs": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "yargs": "^17.5.1"
  },
  "gitHead": "4cf900086db8fd4dbd60d4274e2ac2d4b01127d9"
}
