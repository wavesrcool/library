{
  "extends": "@tsconfig/node16",
  "compilerOptions": {
    "composite": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "declarationMap": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@wavesrcool/dev": ["dev/src"],
    },
  }
 }
